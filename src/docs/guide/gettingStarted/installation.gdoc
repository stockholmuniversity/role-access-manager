# The plugin is published in the Stockholm University grails plugins repository. Add it to your BuildConfig.groovy:
{code}
repositories {
  ...

  grailsRepo "http://svn.it.su.se/grails-plugins/"

  ...
}
{code}

# Install the plugin
Add the following to your BuildConfig.groovy
{code}
  plugins {
    ...

    runtime ':role-access-manager:version'

    ...
  }

{code}
